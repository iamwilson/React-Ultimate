{
  "compilerOptions": {
    "jsx": "react",
    "target": "es6",
    "outDir": "/dist",
    "rootDir": "src",
    "module": "commonjs",
    "removeComments": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "allowSyntheticDefaultImports": true
  },

  "exclude": ["node_modules", "webpack"]
}