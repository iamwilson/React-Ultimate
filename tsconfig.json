{
  "compilerOptions": {
    "jsx": "react",
    "outDir": "dist",
    "target": "esnext",
    "module": "esnext",
    "allowJs": true,
    "sourceMap": true,
    "importHelpers": true,
    "removeComments": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "noImplicitReturns": false,
    "moduleResolution": "node",
    "lib": ["dom", "es5", "es6", "es7"],
    "allowSyntheticDefaultImports": true
  },
  "exclude": ["dist", "node_modules", "webpack"]

}